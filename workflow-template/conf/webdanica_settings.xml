<settings>
  <database>
    <system>hbase-phoenix</system>
    <connection>jdbc:phoenix:narcana-hbase01.statsbiblioteket.dk,narcana-hbase02.statsbiblioteket.dk,narcana-yarn01.statsbiblioteket.dk,narcana-yarn02.statsbiblioteket.dk,narcana-ambari01.statsbiblioteket.dk:2181:/hbase</connection>
  </database>
  <env>prod</env>
  <mail>
    <host>localhost</host>
    <admin>webdanica@localhost</admin>
    <port>25</port>
  </mail>
  <harvesting>
    <maxSingleSeedHarvests>2</maxSingleSeedHarvests>
    <schedule>Once</schedule>
    <template>webdanica_order</template>
    <prefix>webdanica-trial-</prefix>
    <maxbytes>10000</maxbytes>
    <maxobjects>10000</maxobjects>
    <harvestlogDir>/opt/workflows/harvestlogs/</harvestlogDir>
    <harvestMaxTimeInMillis>900000</harvestMaxTimeInMillis>
  </harvesting>
  <seeds>
    <rejectDkUrls>true</rejectDkUrls>
    <ignoredSuffixes>
                <suffix>.jpg</suffix>
                <suffix>.avi</suffix>
                <suffix>.waw</suffix>
                <suffix>.gif</suffix>
                <suffix>.ico</suffix>
                <suffix>.bmp</suffix>                   
                <suffix>.doc</suffix>
                <suffix>.docx</suffix>
                <suffix>.dot</suffix>
                <suffix>.eps</suffix>
                <suffix>.exe</suffix>
                <suffix>.jp2</suffix>           
                <suffix>.jpe</suffix>           
                <suffix>.jpeg</suffix>
                <suffix>.mdb</suffix>           
                <suffix>.mov</suffix>                           
                <suffix>.mp3</suffix>   
                <suffix>.mp4</suffix>           
                <suffix>.mpeg</suffix>                                  
                <suffix>.odt</suffix>
                <suffix>.pdd</suffix>
                <suffix>.pdf</suffix>
                <suffix>.pict</suffix>
                <suffix>.png</suffix>
                <suffix>.psd</suffix>
                <suffix>.rar</suffix>
                <suffix>.raw</suffix>
                <suffix>.rtf</suffix> <!-- ???? -->
                <suffix>.swf</suffix>
                <suffix>.tif</suffix>
                <suffix>.tiff</suffix>             
                <suffix>.wps</suffix>
                <suffix>.xls</suffix>
                <suffix>.css</suffix>
                <suffix>.js</suffix>
    </ignoredSuffixes>
    <ignoredProtocols>
      <protocol>mailto</protocol>
      <protocol>vimeo</protocol>
      <protocol>data</protocol>
      <protocol>ftp</protocol>
    </ignoredProtocols>                  
  </seeds>
  <webapp>
    <crontab>
    <filtering>*/10 * * * *</filtering>
    <harvesting>*/10 * * * *</harvesting>
    <statecaching>0,15,30,45 * * * *</statecaching>
    </crontab>

    <resources>
    <defaultsecuredsetting>false</defaultsecuredsetting>
<resource>
<path>/harvests/</path>
<secured>false</secured>
</resource>

<resource>
<path>/harvest/</path>
<secured>false</secured>
</resource>

<resource>
<path>/domains/</path>
<secured>false</secured>
</resource>

<resource>
<path>/domain/</path>
<secured>false</secured>
</resource>

<resource>
<path>/blacklists/</path>
<secured>false</secured>
</resource>

<resource>
<path>/blacklist/</path>
<secured>false</secured>
</resource>

<resource>
<path>/status/</path>
<secured>false</secured>
</resource>

<resource>
<path>/status/props/</path>
<secured>false</secured>
</resource>

<resource>
<path>/status/dep/</path>
<secured>false</secured>
</resource>

<resource>
<path>/status/threads/</path>
<secured>false</secured>
</resource>

<resource>
<path>/status/progress/</path>
<secured>false</secured>
</resource>

<resource>
<path>/status/log/</path>
<secured>false</secured>
</resource>

<resource>
<path>/status/healthy/</path>
<secured>true</secured>
</resource>
<resource>
<path>/status/sqlquery/</path>
<secured>true</secured>
</resource>
<resource>
<path>/criteriaresult/</path>
<secured>true</secured>
</resource>
<resource>
<path>/criteriaresults/</path>
<secured>true</secured>
</resource>

<resource>
<path>/ingestlogs/</path>
<secured>false</secured>
</resource>

<resource>
<path>/ingestlog/</path>
<secured>false</secured>
</resource>

<resource>
<path>/img/*</path>
<secured>false</secured>
</resource>

<resource>
<path>/js/*</path>
<secured>false</secured>
</resource>

<resource>
<path>/</path>
<secured>false</secured>
</resource>

<resource>
<path>/seed/</path>
<secured>false</secured>
</resource>


<resource>
<path>/seeds/</path>
<secured>false</secured>
</resource>
<!-- %3C%3E = <> %3C%3E -->
<resource>
<path>/seeds/%3Cnumeric%3E/</path>
<secured>false</secured>
</resource>

<resource>
<path>/seeds/%3Cnumeric%3E/dump/</path>
<secured>false</secured>
</resource>

    </resources>
  </webapp>
</settings>
