    <webapp>
        <filtering>
            <maxRecordsProcessedPerRun>50000</maxRecordsProcessedPerRun>
        </filtering>
        <crontab>
            <filtering>*/9 * * * *</filtering>
            <harvesting>*/9 * * * *</harvesting>
            <statecaching>0,15,30,45 * * * *</statecaching>
        </crontab>

        <resources>
            <defaultsecuredsetting>false</defaultsecuredsetting>
            <resource>
                <path>/harvests/</path>
                <secured>false</secured>
            </resource>

            <resource>
                <path>/harvest/</path>
                <secured>false</secured>
            </resource>

            <resource>
                <path>/domains/</path>
                <secured>false</secured>
            </resource>

            <resource>
