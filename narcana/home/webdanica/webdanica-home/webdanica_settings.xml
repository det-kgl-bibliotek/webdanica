<settings>
    <database>
        <system>hbase-phoenix</system>
        <connection>
            jdbc:phoenix:narcana-hbase01.statsbiblioteket.dk,narcana-hbase02.statsbiblioteket.dk,narcana-yarn01.statsbiblioteket.dk,narcana-yarn02.statsbiblioteket.dk,narcana-ambari01.statsbiblioteket.dk:2181:/hbase
        </connection>
    </database>
    <env>prod</env>
    <mail>
        <host>localhost</host>
        <admin>webdanica@localhost</admin>
        <port>25</port>
    </mail>
    <harvesting>
        <maxSingleSeedHarvests>10</maxSingleSeedHarvests>
        <schedule>Once</schedule>
        <template>webdanica_order</template>
        <prefix>webdanica-trial-</prefix>
        <maxbytes>10000000</maxbytes>
        <maxobjects>100</maxobjects>
        <harvestlogDir>/opt/webdanica/harvestlogs/</harvestlogDir>
        <harvestMaxTimeInMillis>900000</harvestMaxTimeInMillis>
    </harvesting>
